<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
    <title>dashboard</title>
</head>
<script>
    function sendDeleteRequest() {
        var url = "http://localhost:5000/api/auth/refresh-token"; // Replace with your API endpoint
    
        // Create a new XMLHttpRequest object
        var xhr = new XMLHttpRequest();
    
        // Configure it to be a DELETE request
        xhr.open("DELETE", url, true);
    
        // Set up the request headers if needed
        // xhr.setRequestHeader("Content-Type", "application/json");
    
        // Set up a callback function to handle the response
        xhr.onload = function () {
            if (xhr.status >= 200 && xhr.status < 300) {
                console.log("DELETE request successful");
                // Handle success
            } else {
                console.error("DELETE request failed with status", xhr.status);
                // Handle error
            }
        };
    
        // Send the DELETE request
        xhr.send();
        
    }
</script>
<body>
    <button onclick="sendDeleteRequest()">Logout</button>

</body>
</html>  